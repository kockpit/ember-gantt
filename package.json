  "dependencies": {
    "ember-cli-babel": "^7.4.2",
    "ember-cli-htmlbars": "^3.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.6.3",
    "bootstrap": "^4.3.1",
    "broccoli-asset-rev": "^2.7.0",
    "ember-animated": "^0.3.2",
    "ember-bootstrap": "^2.5.0",
    "ember-cli": "~3.7.1",
    "ember-cli-addon-docs": "^0.6.4",
    "ember-cli-addon-docs-yuidoc": "^0.2.1",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^1.1.1",
    "ember-cli-deploy-git": "^1.3.3",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-github-pages": "^0.2.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.5",
    "ember-cli-inject-live-reload": "^1.10.2",
    "ember-cli-sass": "^10.0.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.2",
    "ember-cli-uglify": "^2.1.0",
    "ember-code-snippet": "^2.4.0",
