{
  "name": "@kockpit/ember-gantt",
  "version": "1.1.0",
  "description": "ember addon for beautiful gantt charts",
  "keywords": [
    "ember-addon",
    "gantt"
  ],
  "license": "MIT",
  "author": "kockpit.io",
  "directories": {
    "doc": "doc",
    "test": "tests"
