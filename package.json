    "ember-resolver": "^5.1.1",
    "ember-sortable": "^1.12.5",
    "ember-source": "~3.7.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^5.2.0",
    "eslint-plugin-node": "^7.0.1",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.0",
    "sass": "^1.17.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">=2.12.0"
    }
  },
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/kockpit/ember-gantt/issues"
  },
  "homepage": "https://github.com/kockpit/ember-gantt#readme"
